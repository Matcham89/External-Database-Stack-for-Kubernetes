# ==========================================
# External Database Stack - Environment Variables
# ==========================================
# Copy this file to .env and update with your secure credentials
# Generate secure passwords: openssl rand -base64 32

# PostgreSQL Configuration
# Used by: postgres container, postgres-exporter
POSTGRES_USER=postgres
POSTGRES_PASSWORD=changeme_generate_secure_password_here
POSTGRES_DB=homelab

# SeaweedFS S3 Gateway Credentials
# IMPORTANT: Must match credentials in s3-config.json
# Generate with: openssl rand -base64 24
S3_ACCESS_KEY=changeme_s3_access_key_min_20_chars
S3_SECRET_KEY=changeme_s3_secret_key_min_20_chars
